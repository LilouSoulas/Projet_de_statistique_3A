    "# Pipeline SVM : normalisation + SVM\n",
    "svm_pipeline = Pipeline([\n",
    "    ('scaler', StandardScaler()),        \n",
    "    ('svm', SVC(probability=True, random_state=42))  # Modèle SVM avec probabilités\n",
    "])\n",
    "\n",
    "# Grille de paramètres à tester\n",
    "param_grid = {\n",
    "    'svm__kernel': ['linear', 'rbf', 'poly', 'sigmoid'],  # Types de kernel à tester\n",
    "    'svm__C': [0.1, 1, 10, 50, 1000],                            # Paramètre de régularisation\n",
    "    'svm__gamma': ['scale', 'auto', 0.001, 0.01, 0.1, 1]         # Paramètre gamma pour les kernels RBF ou poly\n",
    "}\n",
    "\n",
    "# Définition d'une validation croisée stratifiée à 3 folds\n",
    "cv = StratifiedKFold(n_splits=3, shuffle=True, random_state=42)\n",
    "\n",
    "\n",
    "# Création de GridSearchCV pour tester toutes les combinaisons d'hyperparamètres\n",
    "grid_search = GridSearchCV(\n",
    "    estimator=svm_pipeline,    # Pipeline à évaluer\n",
    "    param_grid=param_grid,     # Grille des hyperparamètres\n",
    "    scoring='precision',       # On optimise la précision\n",
    "    cv=cv,\n",
    "    n_jobs=-1,            \n",
    "    verbose=2\n",
    ")\n",
    "\n",
    "# Entraînement du GridSearchCV sur l'ensemble d'entraînement\n",
    "grid_search.fit(X_train, y_train)\n",
    "\n",
    "# Affichage des meilleurs hyperparamètres trouvés pour le SVM\n",
    "print(\"Meilleurs paramètres trouvés :\", grid_search.best_params_)\n",
    "\n",
    "# Récupération du meilleur modèle SVM\n",
    "best_svm = grid_search.best_estimator_\n",
