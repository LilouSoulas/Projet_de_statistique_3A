    "# On modifie le dataset pour que chaque colonne soit maintenant binaire\n",
    "# Chaque valeur d'une variable catégorielle devient une colonne (One-Hot Encoding)\n",
    "\n",
    "# Liste de toutes les colonnes à encoder\n",
    "categorical_features = X_train.columns.tolist()\n",
    "\n",
    "# Création du transformateur pour l'encodage\n",
    "# handle_unknown='ignore' permet de gérer des catégories présentes uniquement dans le test\n",
    "preprocessor = ColumnTransformer( \n",
    "    # Application de l'One-Hot Encoder à toutes les colonnes catégorielles en une seule fois\n",
    "    transformers=[\n",
    "        ('cat', OneHotEncoder(handle_unknown='ignore', sparse_output=False), categorical_features)\n",
    "    ]\n",
    ")\n",
    "\n",
    "# Application de l'encodage sur l'ensemble d'entraînement\n",
    "X_train_encoded = preprocessor.fit_transform(X_train)\n",
